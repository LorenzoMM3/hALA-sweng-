<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:gwt='urn:import:com.google.gwt.user.client.ui'
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    
    <g:HorizontalPanel spacing="100" ui:field="pagina">
        
    

            <g:VerticalPanel spacing="30" ui:field="ScenariDaCollegare">
                    <g:Label ui:field="MenuLateraleScenari">Menu Scenari:</g:Label>
                    <g:ListBox ui:field="listaScenari"></g:ListBox>
            </g:VerticalPanel>
            
            <g:VerticalPanel spacing="30" ui:field="ScenarioInizialePanel">
                <g:Label ui:field="scenarioIniziale">Scegli lo scenario iniziale:</g:Label>
                <g:ListBox ui:field="menuScenari"></g:ListBox>
                <g:Button ui:field="buttonSettaScenarioIniziale">Setta scenario iniziale</g:Button>
                <g:Label ui:field="messageLabel"></g:Label>
            </g:VerticalPanel>

        
            <g:VerticalPanel spacing="30" ui:field="CollegamentiPanel">
                
                <g:Label>Collega il seguente scenario:</g:Label>
                <g:Label ui:field="testoScenarioDaCollegare"></g:Label>
                <g:ListBox ui:field="scenariConCuiCollegare"></g:ListBox>
                <g:Button ui:field="settaPrecedente">Setta come precedente</g:Button>
                <g:Button ui:field="settaSuccessivo">Setta come successivo</g:Button>

            </g:VerticalPanel>

        
        
        <g:Button ui:field="ScenarioSucc">Passa allo scenario successivo</g:Button>
        <g:Button ui:field="backButton">Back</g:Button>
    </g:HorizontalPanel>
</ui:UiBinder>
