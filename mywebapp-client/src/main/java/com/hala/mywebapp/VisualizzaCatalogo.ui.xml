<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:gwt='urn:import:com.google.gwt.user.client.ui'
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:c="urn:import:com.google.gwt.user.cellview.client">


    <g:VerticalPanel>
        <c:CellTable ui:field="tabella" width="100%" height="100%"/>

        <g:HorizontalPanel>
            <g:Button ui:field="backButton">Back</g:Button>
            <g:TextBox ui:field="nomeRicercaTitolo"></g:TextBox>
            <g:Button ui:field="ricercaTitolo">Ricerca Titolo</g:Button>
            <g:Button ui:field="resettaRicerca">Reset Ricerca</g:Button>
        </g:HorizontalPanel>
    </g:VerticalPanel>
</ui:UiBinder>